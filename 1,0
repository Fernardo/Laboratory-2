#include <iostream>
using namespace std;
int main()
{
int a, b, c;
cout << «Введите a: »;
cin >> a;
cout << «Введите b: »;
cin >> b;
c = a + b;
cout << «Сумма a и b равна » << c << endl;
return 0;
}
